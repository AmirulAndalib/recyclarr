{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "https://raw.githubusercontent.com/recyclarr/recyclarr/master/schemas/config/quality-definition.json",
  "type": "object",
  "additionalProperties": false,
  "required": ["type"],
  "properties": {
    "type": {
      "type": "string"
    },
    "preferred_ratio": {
      "type": "number",
      "default": 1.0,
      "minimum": 0.0,
      "maximum": 1.0
    }
  }
}
